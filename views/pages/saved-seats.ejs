<!DOCTYPE html>
<html>
        <%- include('../partials/head') %>
<body class="wrapper">
        <%- include('../partials/header') %>
    <main>
        <div class="saved-seats">
            <% if (typeof(eventList) !== 'string') { %>
                <% eventList.forEach((event) => { %>
                    <article>
                    <%- include('../partials/event-saved', {event: event}) %>
                    </article>
                <% }) %>
            <% } else { %>
                <p class="no-upcoming-events"><%= eventList %></p>
            <% } %>
        </div>
    </main>
    <%- include('../partials/footer') %>
</body>
</html>